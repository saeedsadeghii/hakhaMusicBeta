    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "file-loader": "^6.2.0",
    "postcss": "^8",
    "prisma": "^5.15.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
